<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Students</title>
    <style>
      table, th, td {
        border: 2px solid black;
      }
    </style>
  </head>
  <body>
<h1> Data Students </h1>
<table style="width:50%">
  <tr>
    <th>No </th>
    <th>First Name </th>
    <th>Last Name </th>
    <!-- <th>Created at </th>
    <th>updated at</th> -->
    <th>Email</th>
  </tr>
<% var i = 1 %>
<% stddata.forEach(function(student) { %>
   <tr>
    <td><%= i %> <% i+=1 %></td>
    <td><%= student.first_name %></td>
    <td><%= student.last_name %></td>
    <td><%= student.email %></td>
    </tr>
<% }) %>
</table>
<br>
<br>

  </body>
</html>
