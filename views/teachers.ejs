<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Teachers</title>
    <style>
      table, th, td {
        border: 1px solid black;
      }
    </style>
  </head>
  <body>
<h1> Data Teachers </h1>
<table>
  <tr>
    <th>Nomor </th>
    <th>First Name </th>
    <th>Last Name </th>
    <th>Created at </th>
    <th>updated at</th>
    <th>Email</th>
  </tr>
<% var i = 1 %>
<% objdata.forEach(function(teacher) { %>
   <tr>
    <td><%= i %> <% i+=1 %></td>
    <td><%= teacher.first_name %></td>
    <td><%= teacher.last_name %></td>
    <td><%= teacher.createdAt %></td>
    <td><%= teacher.updatedAt %></td>
    <td><%= teacher.email %></td>
    </tr>
<% }) %>
</table>
<br>
<br>

  </body>
</html>
